<?xml version="1.0"?>
<robot name="differential_drive_robot">

  <!-- ================= BASE FOOTPRINT ================= -->
  <!-- Ground reference at rear axle center -->
  <link name="base_footprint"/>

  <!-- ================= BASE LINK ================= -->
  <!-- Raised above ground by wheel radius -->
  <link name="base_link">
  </link>

  <joint name="base_footprint_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.23"/>
  </joint>

  <!-- Rear Right Wheel -->
  <joint name="rear_right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rear_right_wheel"/>
    <origin xyz="0 -0.50 0.0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="rear_right_wheel">
    <visual>
      <origin rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.23" length="0.10"/>
      </geometry>
      <material name="yellow">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>

  <!-- Rear Left Wheel -->
  <joint name="rear_left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rear_left_wheel"/>
    <origin xyz="0 0.50 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="rear_left_wheel">
    <visual>
      <origin rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.23" length="0.10"/>
      </geometry>
      <material name="yellow"/>
    </visual>
  </link>

  <!-- ================= FRONT WHEELS ================= -->

  <!-- Front Right Wheel -->
  <joint name="front_right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_right_wheel"/>
    <origin xyz="2.44 -0.435 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="front_right_wheel">
    <visual>
      <origin rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.23" length="0.10"/>
      </geometry>
      <material name="yellow"/>
    </visual>
  </link>

  <!-- Front Left Wheel -->
  <joint name="front_left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_left_wheel"/>
    <origin xyz="2.44 0.435 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="front_left_wheel">
    <visual>
      <origin rpy="1.5708 0 0"/>
      <geometry>
        <cylinder radius="0.23" length="0.10"/>
      </geometry>
      <material name="yellow"/>
    </visual>
  </link>

  <!-- ================= CHASSIS Back ================= -->

  <joint name="chassis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis_Back_box"/>
    <origin xyz="1.0 0 0.8"/>
  </joint>

  <link name="chassis_Back_box">
    <visual>
      <geometry>
        <box size="2.0 0.9 1.60"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
  </link>

   <!-- ================= CHASSIS fr ================= -->

  <joint name="chassis_joint-fr" type="fixed">
    <parent link="base_link"/>
    <child link="chassis_front_box"/>
    <origin xyz="2.20 0 0.8"/>
  </joint>

  <link name="chassis_front_box">
    <visual>
      <geometry>
        <box size="0.45 0.75 1.6"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
  </link>

  <!-- ================= TOP LIDAR ================= -->

  <joint name="top_lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="os_top_sensor"/>
    <origin xyz="1.93 0.0 1.67"/>
  </joint>

  <link name="os_top_sensor">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.13"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
  </link>

  <!-- ================= front SIDE LIDAR ================= -->

  <!-- <joint name="front side_lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="os_front_sensor"/>
    <origin xyz="2.66 0.7 0.42" rpy="0 0 0.7"/>
  </joint>

  <link name="os_front_sensor">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link> -->

   <joint name="front side_lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="os_sensor"/>
    <origin xyz="2.66 0.7 0.42" rpy="0 0 0.7"/>
  </joint>

  <link name="os_sensor">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  
 <!-- ================= back SIDE LIDAR ================= -->

  <joint name="back side_lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="os_back_sensor"/>
    <origin xyz="-0.53 -0.61 0.42" rpy="0 0 3.52"/>
  </joint>

  <link name="os_back_sensor">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>

  <!-- ================= CHASSIS fr-1 ================= -->

  <joint name="front_lidar_mount" type="fixed">
    <parent link="base_link"/>
    <child link="front_lidar_mount"/>
    <origin xyz="2.40 0.7 0.365"/>
  </joint>

  <link name="front_lidar_mount">
    <visual>
      <geometry>
        <box size="0.55 0.05 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>

  <!-- ================= CHASSIS fr-1 ================= -->

  <joint name="front_lidar_mount-1" type="fixed">
    <parent link="base_link"/>
    <child link="front_lidar_mount-1"/>
    <origin xyz="2.15 0.4 0.365"/>
  </joint>

  <link name="front_lidar_mount-1">
    <visual>
      <geometry>
        <box size="0.05 0.55 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>

   <!-- ================= CHASSIS back-1 ================= -->

  <joint name="Back_lidar_mount" type="fixed">
    <parent link="base_link"/>
    <child link="Back_lidar_mount"/>
    <origin xyz="-0.25 -0.61 0.365"/>
  </joint>

  <link name="Back_lidar_mount">
    <visual>
      <geometry>
        <box size="0.65 0.05 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>

  <!-- ================= CHASSIS back-1 ================= -->

  <joint name="Back_lidar_mount-1" type="fixed">
    <parent link="base_link"/>
    <child link="Back_lidar_mount-1"/>
    <origin xyz="0.05 -0.45 0.365"/>
  </joint>

  <link name="Back_lidar_mount-1">
    <visual>
      <geometry>
        <box size="0.05 0.35 0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>

  <!-- ================= IMU ================= -->

  <joint name="imu_joint" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin xyz="0 0 0.6" rpy="0 0 0"/>
  </joint>

  <link name="imu_link">
    <visual>
      <geometry>
        <box size="0.08 0.05 0.03"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
  </link>

  <!-- ================= GPS ================= -->

  <joint name="gps_joint" type="fixed">
    <parent link="base_link"/>
    <child link="gps_link"/>
    <origin xyz="1.36 0.0 1.67" rpy="0 0 0"/>
  </joint>

  <link name="gps_link">
    <visual>
      <geometry>
        <cylinder radius="0.02" length="0.25"/>
      </geometry>
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
  </link>




</robot>